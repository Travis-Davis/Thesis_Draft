issn = {08948755},
journal = {Journal of Climate},
keywords = {Arctic,Atm/Ocean Structure/Phenomena,Atmosphere-ocean interaction,Circulation/Dynamics,Coupled models,Geographic location/entity,Models and modeling,Oceanic mixed layer,Regional models,Wind},
number = {3},
pages = {975--994},
title = {{Winter atmospheric buoyancy forcing and oceanic response during strong wind events around southeastern Greenland in the Regional Arctic System Model (RASM) for 1990-2010}},
volume = {29},
year = {2016}
}
