number = {2},
pages = {107--129},
primaryClass = {Timco, G. W., {\&} Weeks, W. F. (2010). A review of the engineering properties of sea ice. Cold Regions Science and Technology, 60(2), 107–129. doi:10.1016},
publisher = {Elsevier B.V.},
