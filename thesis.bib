title = {{On the numerical simulation of the sea ice ridging process}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=1991JGR....96.4809H{\&}link{\_}type=ABSTRACT{\%}0Apapers2://publication/doi/10.1029/90JC02375},
