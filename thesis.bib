doi = {10.1103/PhysRevLett.110.174301},
eprint = {1210.2745},
file = {:Users/travisdavis/Library/Application Support/Mendeley Desktop/Downloaded/Galley - 2013 - Classical mechanics of nonconservative systems.pdf:pdf},
isbn = {0031-9007},
issn = {00319007},
journal = {Physical Review Letters},
