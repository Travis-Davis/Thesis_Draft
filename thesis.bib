title = {{Molecular-dynamics simulation of clustering processes in sea-ice floes}},
volume = {84},
year = {2011}
}
@article{DuVivier2016,
abstract = {{\textcopyright} 2016 American Meteorological Society.Strong, mesoscale tip jets and barrier winds that occur along the southeastern Greenland coast have the potential to impact deep convection in the Irminger Sea. The self-organizing map (SOM) training algorithm was used to identify 12 wind patterns that represent the range of winter [November-March (NDJFM)] wind regimes identified in the fully coupled Regional Arctic System Model (RASM) during 1990-2010. For all wind patterns, the ocean loses buoyancy, primarily through the turbulent sensible and latent heat fluxes; haline contributions to buoyancy change were found to be insignificant compared to the thermal contributions. Patterns with westerly winds at the Cape Farewell area had the largest buoyancy loss over the Irminger and Labrador Seas due to large turbulent fluxes from strong winds and the advection of anomalously cold, dry air over the warmer ocean. Similar to observations, RASM simulated typical ocean mixed layer depths (MLD) of approximately 400 m throughout the Irminger basin, with individual years experiencing MLDs of 800 m or greater. The ocean mixed layer deepens over most of the Irminger Sea following wind events with northerly flow, and the deepening is greater for patterns of longer duration. Seasonal deepest MLD is strongly and positively correlated to the frequency of westerly tip jets with northerly flow.},
author = {DuVivier, Alice K. and Cassano, John J. and Craig, Anthony and Hamman, Joseph and Maslowski, Wieslaw and Nijssen, Bart and Osinski, Robert and Roberts, Andrew},
